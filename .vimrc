syntax on
colorscheme darkblue
set autoindent
set number
set incsearch
set showmode
set showmatch
set showcmd
set wrap
set title
set list
set listchars=tab:>-,trail:-,eol:\

" search
set wrapscan
set ignorecase
" /search=>case insensitive /Search=>case sensitive
set smartcase
set hlsearch

"
set wildmenu
set wildmode=list:full

" swap / backup file
set nobackup
set noswapfile
